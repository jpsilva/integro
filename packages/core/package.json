{
  "name": "integro",
  "version": "0.3.0",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./browser": "./dist/browser.js"
  },
  "files": ["dist"],
  "license": "MIT",
  "repository": "jpsilva/integro",
  "keywords": ["web", "app", "server", "e2e", "router", "api", "middleware", "rack"],
  "scripts": {
    "build": "bun build:server && bun build:browser && bun build:types",
    "build:browser": "bun build src/browser.ts --outdir=dist --target browser",
    "build:server": "bun build src/index.ts --outdir=dist --target node",
    "build:types": "tsc",
    "prepublishOnly": "rm -rf dist && rm tsconfig.tsbuildinfo; bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest",
    "del-cli": "^5.1.0",
    "type-fest": "^4.15.0",
    "typescript": "^5.0.0",
    "undici-types": "^6.14.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@jsonjoy.com/json-pack": "^1.0.2",
    "json-joy": "^15.4.0",
    "msgpackr": "^1.10.1"
  }
}
