{
  "name": "integro",
  "version": "0.3.2",
  "main": "dist/index.js",
  "browser": "dist/client.js",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js"
  },
  "files": ["dist"],
  "license": "MIT",
  "repository": "jpsilva/integro",
  "keywords": ["web", "app", "server", "e2e", "router", "api", "middleware", "rack"],
  "scripts": {
    "build": "bun build:server && bun build:client && bun build:types",
    "build:client": "bun build src/client.ts --outdir=dist --target browser",
    "build:server": "bun build src/index.ts --outdir=dist --target node",
    "build:types": "tsc",
    "lint": "eslint .",
    "prepublishOnly": "rm -rf dist && rm tsconfig.tsbuildinfo; bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:types": "tstyche"
  },
  "devDependencies": {
    "@integro/eslint-config": "workspace:*",
    "@types/bun": "latest",
    "@types/node": "latest",
    "del-cli": "^5.1.0",
    "get-port": "^7.1.0",
    "tstyche": "^1.1.0",
    "type-fest": "^4.15.0",
    "typescript": "^5.0.0",
    "undici-types": "^6.14.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "msgpackr": "^1.10.1"
  }
}
